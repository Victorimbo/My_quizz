{% extends 'base.html.twig' %}

{% block title %}Hello SuperAdminController!
{% endblock %}

{% block body %}
	<style>
		.example-wrapper {
			margin: 1em auto;
			max-width: 800px;
			width: 95%;
			font: 18px / 1.5 sans-serif;
		}
		.example-wrapper code {
			background: #F5F5F5;
			padding: 2px 6px;
		}
	</style>

	<div class="example-wrapper">
		<table>
			<tr>
				<td>Name of quizz</td>
				<td>Edit</td>
				<td>Delete</td>
			</tr>
			{% for item in data %}
				<tr>
					<td>{{ item.name }}</td>
					<td><a href='/admin/categories/edit/{{ item.id }}'>Edit</a></td>
					<td><a href='/admin/categories/delete/{{ item.id }}'>Delete</a></td>
					
				</tr>
			{% endfor %}
		</table>
	</div>
{% endblock %}
